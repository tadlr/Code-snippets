{# page-bk6--talk-to-us #}
{% extends "page-sk-block-base.twig" %}

{% block block_base %}
  {% set attributes = attributes.addClass('inner-pages-contact-section contact-block prel offset-x-desktop-304').setAttribute('data-scroll-section-id', 'section2').setAttribute('data-link-scroll-target-section-id', 'guide') %}
  {{ parent() }}
{% endblock %}

{% block block_content %}
  {% set fieldId = field.contact_block %}
  {% set block_field = get_fields(fieldId) %}
  {% if lang == 'es' %}
    {% set label = 'Cerrar formulario emergente' %}
    {% set cta = 'Programa una devoluci√≥n de llamada' %}
  {% else %}
    {% set label = 'Close form popup' %}
    {% set cta = 'Schedule a callback' %}
  {% endif %}

  {% if fieldId and block_field %}
    <div class='op-0 o-rel'>
      <div
        class='bg-image b-lazy dont-show'
        data-src='{{get_template_directory_uri() }}/assets/media/images/inner/contact-section-bgr.webp'
      ></div>

      <div class='op-0 container-fw prel z-3'>
        <div class='background-flower-path-patterns'>
          <svg viewbox='0 0 451.3 451.3' class='background-flower-path-svg'>
            <path class="st0" d="M225.7 225.7c124.4 0 225.1 100.8 225.1 225.2-124.3-.1-225.1-100.9-225.1-225.2z"></path>
            <path class="st0" d="M450.8.5c0 124.4-100.8 225.2-225.1 225.2C225.7 101.3 326.5.5 450.8.5z"></path>
            <path class="st0" d="M225.7 225.7c0 124.4-100.8 225.2-225.2 225.2 0-124.4 100.8-225.2 225.2-225.2z"></path>
            <path class="st0" d="M.5.5c124.4 0 225.2 100.8 225.2 225.2C101.3 225.7.5 124.9.5.5z"></path>
          </svg>
          <svg viewbox='0 0 451.3 451.3' class='background-flower-path-svg'>
            <path class="st0" d="M225.7 225.7c124.4 0 225.1 100.8 225.1 225.2-124.3-.1-225.1-100.9-225.1-225.2z"></path>
            <path class="st0" d="M450.8.5c0 124.4-100.8 225.2-225.1 225.2C225.7 101.3 326.5.5 450.8.5z"></path>
            <path class="st0" d="M225.7 225.7c0 124.4-100.8 225.2-225.2 225.2 0-124.4 100.8-225.2 225.2-225.2z"></path>
            <path class="st0" d="M.5.5c124.4 0 225.2 100.8 225.2 225.2C101.3 225.7.5 124.9.5.5z"></path>
          </svg>
          <svg viewbox='0 0 451.3 451.3' class='background-flower-path-svg'>
            <path class="st0" d="M225.7 225.7c124.4 0 225.1 100.8 225.1 225.2-124.3-.1-225.1-100.9-225.1-225.2z"></path>
            <path class="st0" d="M450.8.5c0 124.4-100.8 225.2-225.1 225.2C225.7 101.3 326.5.5 450.8.5z"></path>
            <path class="st0" d="M225.7 225.7c0 124.4-100.8 225.2-225.2 225.2 0-124.4 100.8-225.2 225.2-225.2z"></path>
            <path class="st0" d="M.5.5c124.4 0 225.2 100.8 225.2 225.2C101.3 225.7.5 124.9.5.5z"></path>
          </svg>
          <svg viewbox='0 0 451.3 451.3' class='background-flower-path-svg'>
            <path class="st0" d="M225.7 225.7c124.4 0 225.1 100.8 225.1 225.2-124.3-.1-225.1-100.9-225.1-225.2z"></path>
            <path class="st0" d="M450.8.5c0 124.4-100.8 225.2-225.1 225.2C225.7 101.3 326.5.5 450.8.5z"></path>
            <path class="st0" d="M225.7 225.7c0 124.4-100.8 225.2-225.2 225.2 0-124.4 100.8-225.2 225.2-225.2z"></path>
            <path class="st0" d="M.5.5c124.4 0 225.2 100.8 225.2 225.2C101.3 225.7.5 124.9.5.5z"></path>
          </svg>
        </div>
        <div class='prel row'>
          <div class='txt-content'>
            <div class='col-sm-12 col-md-6 single-block txt-content-block column'>
              <div class='column-inner'>
                <div class='w-500'>
                  <p class='txt-size-12 lh-1 ff-libre upper fw-400 ltr-spc-pos-5'>
                    {{ block_field.left_subheading }}
                  </p>
                  <h2 class='txt-size-60 txt-size-48-m c-white  fw-700 ltr-spc-pos-0_5 talk-left-heading heading-anim'>
                    {{ block_field.left_main_heading }}
                  </h2>
                  <div class='buttons-wrap'>
                    {% set phone = sm_toll_free_number(block_field.phone) %}
                    <a href='tel:{{phone}}' class='contact-section-btn txt-size-24 lh-1_25 c-white fw-700'>
                      <img
                        class='contact-btn-icon b-lazy'
                        alt=''
                        role='presentation'
                        width='60'
                        height='60'
                        data-src='{{ get_template_directory_uri() }}/assets/media/images/pictograms/talk-to-us-icon.webp'
                      >
                      <span class='btn-txt text-white'>
                        {{ block_field.phone_link_text }}
                      </span>
                      {% if (get_the_id() == 11835) %}
                        <span style='display: none;'>
                          {{ apply_filters("ucomm_format_gfn", block_field.phone) }}
                        </span>
                      {% endif %}
                    </a>
                    <button class='contact-section-btn txt-size-24 lh-1_25 c-white fw-700 callback-popup-btn'>
                      <img
                        class='contact-btn-icon b-lazy'
                        alt=''
                        role='presentation'
                        width='60'
                        height='60'
                        data-src='{{ get_template_directory_uri() }}/assets/media/images/pictograms/callback-icon.webp'
                      >
                      <span class='btn-txt text-white'>
                        {{ block_field.contacts_text }}
                      </span>
                    </button>
                  </div>
                  <div class='txt-post-rtf fw-400 c-white  lh-1_66 ltr-spc-pos-0_2 talk-left-content'>
                    <p>
                      {{ block_field.left_text_content }}
                    </p>
                  </div>
                </div>
              </div>
            </div>
            <div class='col-sm-12 col-md-6 single-block contact-block prel column txt-18'>
              <div class='column-inner txt-18'>
                <div class='w-500'>
                  <p class='txt-size-12 lh-1 ff-libre upper fw-400 ltr-spc-pos-5'>
                    {{ block_field.right_subheading }}
                  </p>
                  <h2 class='txt-size-60 txt-size-48-m c-white  fw-700 ltr-spc-pos-0_5 talk-right-heading heading-anim'>
                    {{ block_field.right_main_heading }}
                  </h2>
                  <div class='txt-post-rtf fw-400 c-white  lh-1_66 ltr-spc-pos-0_2 talk-right-content'>
                    <p>
                      {{ block_field.right_text_content }}
                    </p>
                    {{ block_field.bullet_list }}
                  </div>
                  <div class='newsletter-form col prel'>
                    {% set spanish_pages = get_field('spanish_pages', 'option') %}
                    {% if post.id in spanish_pages or lang == 'es' %}
                      <script>
                        hbspt.forms.create({
                          region: 'na1',
                          portalId: '4438792',
                          formId: '38ad7a0c-4b18-4299-86c0-6c83fb91031f',
                        });
                      </script>
                    {% else %}
                      <script>
                        hbspt.forms.create({
                          region: 'na1',
                          portalId: '4438792',
                          formId: 'd74be8e6-676d-43a4-a3cf-5f796747705f',
                        });
                      </script>
                    {% endif %}
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  {% endif %}
{% endblock %}
